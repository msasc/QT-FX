<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>System library strings</comment>

	<!-- Alert options. -->

	<entry key="alertOptionAccept">Accept</entry>
	<entry key="alertOptionApply">Apply</entry>
	<entry key="alertOptionCancel">Cancel</entry>
	<entry key="alertOptionClose">Close</entry>
	<entry key="alertOptionFinish">Finish</entry>
	<entry key="alertOptionIgnore">Ignore</entry>
	<entry key="alertOptionNext">Next</entry>
	<entry key="alertOptionNo">No</entry>
	<entry key="alertOptionOk">Ok</entry>
	<entry key="alertOptionOpen">Open</entry>
	<entry key="alertOptionPrevious">Previous</entry>
	<entry key="alertOptionRetry">Retry</entry>
	<entry key="alertOptionYes">Yes</entry>
	
	<entry key="alertTitleError">Error</entry>
	<entry key="alertTitleInformation">Information</entry>
	<entry key="alertTitlePlain">Plain</entry>
	<entry key="alertTitleQuestion">Question</entry>
	<entry key="alertTitleWarning">Warning</entry>
	
	<!-- Several widely used tokens. -->
	
	<entry key="tokenCounting">Counting</entry>
	<entry key="tokenDay">Day</entry>
	<entry key="tokenDays">Days</entry>
	<entry key="tokenElapsed">Elapsed</entry>
	<entry key="tokenEnd">End</entry>
