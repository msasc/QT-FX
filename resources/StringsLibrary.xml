	<entry key="tokenProcessing">Processing</entry>
	<entry key="tokenRemaining">Remaining</entry>
	<entry key="tokenSecond">Second</entry>
	<entry key="tokenSeconds">Seconds</entry>
	<entry key="tokenStart">Start</entry>
	<entry key="tokenStarted">Started</entry>
	<entry key="tokenTime">Time</entry>
	
	<!-- Several widely used tool tips. -->
	
	<entry key="tooltipStart">Start the current task</entry>
	<entry key="tooltipCancel">Cancel (stop) the current task</entry>
	<entry key="tooltipErrorInfo">View detailed error information</entry>
	
	<!-- Task messages. -->
	
	<entry key="taskCancelled">Cancelled</entry>
	<entry key="taskFailed">Failed</entry>
	<entry key="taskSucceeded">Succeeded</entry>
	<entry key="taskParallel">Parallel processing...</entry>

